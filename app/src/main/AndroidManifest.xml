        </activity>
        <!--activity android:name="com.THLight.USBeacon.Sample.ui.UIBeaconData" android:screenOrientation="portrait" /-->


        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <receiver
            android:name="tw.com.hismax.test.hospinfosys.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="tw.com.hismax.test.hospinfosys" />
            </intent-filter>
        </receiver>
        <service android:name="tw.com.hismax.test.hospinfosys.GcmIntentService" />
        <activity android:name=".Logout" />
        <service android:name=".UIMain" />
        <activity android:name=".Web_list" />
